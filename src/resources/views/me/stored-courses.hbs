<div class="mt-4">
  <h3>Khóa học của tôi</h3>

  <table class="table table-dark mt-4">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Tên khóa học</th>
        <th scope="col">Trình đồ</th>
        <th scope="col" colspan="2">Thời gian tạo</th>
      </tr>
    </thead>
    <tbody>
      {{#each courses}}
      <tr>
        <th scope="row">{{sum @index 1}}</th>
        <td>{{this.name}}</td>
        <td>{{this.level}}</td>
        <td>{{this.createAt}}</td>
        <td>
          <a href="/courses/{{this._id}}/edit" class="btn btn-warning">Sửa</button>
            <a href="" class="btn btn-warning">Xóa</button>
        </td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>